{
  "private": true,
  "name": "air-hockey",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev-server": "nodemon .",
    "build-server": "tsc",
    "run-server": "node --experimental-specifier-resolution=node dist-server/index-server.js",
    "run-bot": "node --experimental-specifier-resolution=node dist-server/index-bot.js",
    "lint": "npx eslint src --fix",
    "prettier": "npx prettier src --check --write",
    "format": "npm run prettier && npm run lint"
  },
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "@types/matter-js": "^0.18.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "alea": "^1.0.1",
    "compete-client": "*",
    "compete-server": "*",
    "compete-utils": "*",
    "howler": "^2.2.3",
    "matter-js": "^0.18.0",
    "pixi.js": "^7.0.4",
    "simplex-noise": "^4.0.1",
    "trigfills": "^0.9.2",
    "ws": "^8.11.0"
  }
}
